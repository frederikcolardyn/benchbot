<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.hwbot</groupId>
	<artifactId>cpuid</artifactId>
	<version>1.2.0</version>
	<packaging>jar</packaging>
	<url>http://hwbot.org</url>
	<name>HWBOT Bench CPUID native libraries</name>
	<organization>
		<name>COLARDYN IT GCV</name>
		<url>http://colardyn-it.be/</url>
	</organization>

	<scm>
		<developerConnection>scm:git:https://github.com/frederikcolardyn/benchbot.git/cpuid</developerConnection>
	</scm>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.0</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<encoding>utf-8</encoding>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<ciManagement>
		<notifiers>
			<notifier>
				<type>mail</type>
				<configuration>
					<address>frederikcolardyn@gmail.com</address>
				</configuration>
			</notifier>
		</notifiers>
	</ciManagement>
	<developers>
		<developer>
			<name>Frederik Colardyn</name>
			<email>frederikcolardyn@hwbot.org</email>
			<organization>COLARDYN IT</organization>
			<roles>
				<role>Manager</role>
				<role>Java Developer</role>
			</roles>
			<timezone>+1</timezone>
		</developer>
		<developer>
			<name>Pieter-Jan Plaisier</name>
			<email>pieterjanplaisier@hwbot.org</email>
			<organization>COLARDYN IT</organization>
			<roles>
				<role>Analyst</role>
				<role>Tester</role>
			</roles>
			<timezone>+1</timezone>
		</developer>
	</developers>
	<inceptionYear>2013</inceptionYear>

	<properties>
		<maven.test.failure.ignore>true</maven.test.failure.ignore>
		<project.build.sourceEncoding>utf-8</project.build.sourceEncoding>
	</properties>

</project>
